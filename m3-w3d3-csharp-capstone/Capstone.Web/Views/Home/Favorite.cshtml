@model List<Capstone.Web.Models.ParkSurveyCount>

@{
    ViewBag.Title = "Index";
}
@*<h1>Favorites</h1>*@
<section>
    <table>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <a class=" parklinks" href="@Url.Action("Detail", "Home", new { id = item.ParkCode})">
                        @Html.DisplayFor(modelItem => item.ParkName)
                    </a>
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SurveyCount)
                </td>
            </tr>
        }
    </table>
</section>